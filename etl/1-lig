<div class="block_ad ad_floating_bar block clearfix">
  <div id='div-gpt-ad-1325073145951-2' style='width:1000px; height:50px;'>
<script type='text/javascript'>
googletag.cmd.push(function() { googletag.display('div-gpt-ad-1325073145951-2'); });
</script>
<span class="close">&nbsp;</span>
</div>

<script type="text/javascript">
(function(){
  var el = $$('.ad_floating_bar')[0];
  if (el) {
    var cookie_name = 'sw_no_ad_floating_bar';
    if (!Cookie.get(cookie_name)) {
      googletag.cmd.push(function() { el.style.display = 'block'; });

      Element.select(el, '.close')[0].observe('click', function(evt){
        el.style.display = 'none';

        var date = new Date();
        date.setTime(date.getTime()+(24*60*60*1000)); // 24h

        Cookie.set(cookie_name, 1, {expires: date.toUTCString(), path: '/', domain: Config.get('base_domain')});
        Event.stop(evt);
      });
    }
  }
})();
</script>
