      <a href="/" class="t_l">Today</a>
      <a href="/" class="dropdown-button" id="today-dropdown-button">#</a>    
      <div class="dropdown cal-dropdown" style="display: none;" id="today-dropdown">
        <h2>Calendar</h2>
        <div class="calendar-container clearfix">
  <div id="dropdown-cal" class="calendar"></div>
</div>

<script type="text/javascript" charset="utf-8">
  /* <![CDATA[ */
  (function() {
    Object.extend(Date.prototype, {
      monthnames: dateFormat.i18n.monthNames.slice(0, 11),
      short_monthnames: dateFormat.i18n.monthNames.slice(11, 21),
      daynames: dateFormat.i18n.dayNames.slice(0, 7),
      short_daynames: dateFormat.i18n.dayNames.slice(7, 13)
    });

    var changeHandler = function(date) {
      var year = date.getFullYear().toString(),
          month = (date.getMonth() + 1).toString(),
          day = date.getDate().toString(),
          url,
          url_prefix = '/matches/',
          url_suffix = '';
      var today = new Date();
      var today_year = today.getFullYear().toString(),
          today_month = (today.getMonth() + 1).toString(),
          today_day = today.getDate().toString(),
          today_url = '';

      if (today_url != '' && year == today_year && month == today_month && day == today_day) {
        window.location.href = today_url;
      } else {
        if (month.length === 1) month = '0' + month;
        if (day.length === 1) day = '0' + day;
        url = url_prefix + year + '/' + month + '/' + day + '/';
        if (url_suffix != '' && url_suffix != '/') {
          url += url_suffix;
        }
        window.location.href = url;
      }
    };

    var cal_initialized = false;
    function initializeCalendar(){
      if (cal_initialized == false) {
        var cal = new scal('dropdown-cal', changeHandler, {
          dayheadlength: 3,
          highlightselected: ('2012' != ''),
          year: parseInt('2012'),
          month: parseInt('11'),
          day: parseInt('27')
        });
        cal_initialized = true;
      }
    }

    if (1){
      $('today-dropdown-button').observe('click', initializeCalendar);
    }
    else{
      initializeCalendar();
    }

  })();
  /* ]]> */
</script>
